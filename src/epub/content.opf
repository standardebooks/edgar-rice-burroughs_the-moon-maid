		<dc:title id="title">The Moon Maid</dc:title>
		<meta property="file-as" refines="#title">Moon Maid, The</meta>
		<dc:subject id="subject-1">Science fiction</dc:subject>
		<dc:subject id="subject-2">Moon -- Fiction</dc:subject>
		<dc:subject id="subject-3">Human-alien encounters -- Fiction</dc:subject>
